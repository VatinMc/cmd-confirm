  "authors": [ "VatinMC" ],
  "contact": {
    "repo": "https://github.com/vatinmc/cmd-confirm",
    "homepage": "https://github.com/vatinmc/cmd-confirm",
    "issues": "https://github.com/VatinMc/cmd-confirm/issues"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/cmd-confirm/icon.png",
